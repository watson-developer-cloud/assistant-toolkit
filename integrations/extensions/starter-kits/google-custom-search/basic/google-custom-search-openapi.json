                                "type": "string"
                              },
                              "outputEncoding": {
                                "type": "string"
                              },
                              "safe": {
                                "type": "string"
                              },
                              "cx": {
                                "type": "string"
                              }
                            }
                          }
                        },
                        "nextPage": {
