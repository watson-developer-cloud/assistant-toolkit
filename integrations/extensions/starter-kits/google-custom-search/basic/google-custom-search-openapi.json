    "version": "1.0"
  },
  "servers": [
    {
      "url": "https://www.googleapis.com"
    }
  ],
  "components": {
    "securitySchemes": {
      "apiKeyAuth": {
        "type": "apiKey",
        "name": "key",
        "in": "query"
      }
    }
  },
  "security": [
    {
      "apiKeyAuth": []
    }
